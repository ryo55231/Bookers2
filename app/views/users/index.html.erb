<div class="container">
<div class="row">
  <div class="col-md-3" style="background-color: orange;">

          <!-- 部分テンプレートコードがここに入る -->
          <%= render 'books/list', user: @user ,book: @book%>
    </div>
     <div class="col-md-8 offset-md-1">
<h1>Users</h1>

 <table>
    <tr>
    <th>image</th>
　　<th>name</th>
　　<th colspan="3"></th>
   </tr>

<% @users.each do |user| %>
<tr>
  <td><%= image_tag @user.get_profile_image(100,100) %></td>
  <td><%= user.name %></td>
  </tr>
  <% end %>
  </table>
</div>
</div>
</div>
